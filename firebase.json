{
  "firestore": {
    "database": "focusflowv2",
    "location": "southamerica-west1",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "source": "app-functions",
    "runtime": "nodejs20",
    "predeploy": [
      "npm --prefix app-functions run build"
    ],
    "codebase": "default"
  },
  "apphosting": {
    "backendId": "focusflow-staging",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ],
    "alwaysDeployFromSource": true
  },
  "hosting": {
    "site": "studio-808864941-b51ba",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "focusflow-staging",
          "region": "us-central1"
        }
      }
    ]
  },
  "storage": {
    "rules": "storage.rules",
    "bucket": "studio-808864941-b51ba.firebasestorage.app"
  },
  "emulators": {
    "apphosting": {
      "port": 5002,
      "rootDirectory": "./",
      "startCommand": "npm run dev"
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true,
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "database": {
      "port": 9000
    },
    "hosting": {
      "port": 5000
    },
    "storage": {
      "port": 9199
    },
    "dataconnect": {
      "port": 9399,
      "dataDir": "dataconnect/.dataconnect/pgliteData"
    },
    "tasks": {
      "port": 9499
    }
  }
}
