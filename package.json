{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0 --turbopack",
    "dev:setup": "tsx scripts/setup-dev-environment.ts",
    "dev:seed": "tsx scripts/seed-dev-data.ts",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "emu:pre": "npx kill-port 4013 || true && npx kill-port 4400 || true && npx kill-port 8080 || true && npx kill-port 9099 || true && npx kill-port 9199 || true && npx kill-port 5000 || true && npx kill-port 5001 || true && npx kill-port 5002 || true",
    "dev:all": "npm run emu:pre && firebase emulators:start --only hosting,firestore,auth,storage --import ./.emulator-data --export-on-exit",
    "emu:stop": "firebase emulators:stop || true",
    "emu:clean": "rm -rf ./.emulator-data || true"
  },
  "dependencies": {
